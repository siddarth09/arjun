<launch>
  
  <arg name="x_pos" default="-17.7"/>
  <arg name="y_pos" default="-3.2"/>
  <arg name="z_pos" default="0.0"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find arjun_description)/world/maze2.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>  


  <param command="$(find xacro)/xacro $(find arjun_description)/urdf/arjun.xacro" name="robot_description"/>
  <node args="-param robot_description -urdf -model arjun" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"/>

</launch>
